extends layout
block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Gestão de Listas de Compras
    .w3-container
      table.w3-table-all
        tr
          th ID
          th Username
          th Scope Content
          th Operações
        each i in inquiricoes
          tr
            td= i._id
            td= i.Username
            td= i.ScopeContent
            //td= l.produtos.length
            td
              button.w3-btn.w3-round.w3-indigo.w3-margin-right 
                a(href="#") C
              button.w3-btn.w3-round.w3-indigo.w3-margin-right  
                //a(href="/retrieveList/" + l._id) R
                a(href="#") R
              button.w3-btn.w3-round.w3-indigo.w3-margin-right  
                a(href="#") U 
              button.w3-btn.w3-round.w3-indigo.w3-margin-right  
                a(href="#") D
              
    footer.w3-container.w3-teal
      h5 Generated by shopList::EngWeb2023 in #{d}
